#CHIP 18F26K83, 31k
#CONFIG OSC=LFINTOSC


#OPTION Explicit

Dir porta Out

'Select LTINTOSC as the source and 128 prescaler
T0CON1 = 0x87
'Set to 1 sec timer
TMR0H = 0xF7

On Interrupt timer0overflow Call isr
StartTimer 0

Do

Loop


Sub isr
    'Toggle an LED
    porta.1 = !porta.1
End Sub
